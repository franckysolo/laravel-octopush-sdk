{
    "name": "franckysolo/laravel-octopush-sdk",
    "description": "Laravel plugin for octopush-sdk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "franckysolo",
            "email": "franckysolo@gmail.com"
        }
    ],
    "autoload": {
        "classmap": ["src/"],
        "psr-4": {
            "OctopushSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "franckysolo/octopush-sdk": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OctopushSms\\OctopushServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
}
